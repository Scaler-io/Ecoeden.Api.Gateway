{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ReverseProxy": {
    "Clusters": {
      "catalogue": {
        "Destinations": {
          "catalogueApi": {
            "Address": "http://Ecoeden.Catalogue.Api:8080"
          }
        }
      },
      "search": {
        "Destinations": {
          "searchApi": {
            "Address": "http://Ecoeden.Search.Api:8080"
          }
        }
      },
      "user": {
        "Destinations": {
          "userApi": {
            "Address": "http://Ecoeden.User.Api:80"
          }
        }
      },
      "inventory": {
        "Destinations": {
          "inventoryApi": {
            "Address": "http://Ecoeden.Inventory.Api:80"
          }
        }
      }
    }
  },
  "AllowedHosts": "*"
}
